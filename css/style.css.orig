@media (min-width: 1200px) {
  .container {
    width: 1200px; } }
/*nav bar css fix*/
.navbar-fixed-bottom, .navbar-fixed-top {
  padding: 10px 30px; }
  @media (max-width: 1399px) {
    .navbar-fixed-bottom, .navbar-fixed-top {
      padding: 10px 30px; } }

